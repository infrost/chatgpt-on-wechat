{
    "version": 2,
    "builds": [
        {
            "src": "./app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
    ],
    "build": {
        "env": {
          "VERCEL_BUILD_TIMEOUT": "600"
        }
    }
}
